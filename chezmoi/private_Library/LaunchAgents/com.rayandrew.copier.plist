<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.rayandrew.copier.plist</string>
    <key>ProgramArguments</key>
    <array>
        <string>/Users/rayandrew/bin/copier</string>
        <string>-v</string>
    </array>
    <key>StartInterval</key>
    <integer>300</integer>
    <key>RunAtLoad</key>
    <true/>
    <key>StandardErrorPath</key>
    <string>/tmp/com.rayandrew.copier.err</string>
    <key>StandardOutPath</key>
    <string>/tmp/com.rayandrew.copier.out</string>
</dict>
</plist>
